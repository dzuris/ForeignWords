<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Colors.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    <!-- Override of SystemColors for DataGrid-->
    <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="Green"/>

    <SolidColorBrush x:Key="DefaultWindowBackgroundBrush" Color="White"/>

    <SolidColorBrush x:Key="DefaultForegroundBrush" Color="{StaticResource DefaultForegroundColor}"/>
    <SolidColorBrush x:Key="DefaultBackgroundBrush" Color="{StaticResource DefaultBackgroundColor}"/>
    
    <SolidColorBrush x:Key="DefaultBorderBrush" Color="{StaticResource DefaultBorderColor}"/>

    <SolidColorBrush x:Key="DisabledBorderBrush" Color="Blue"/>
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="GreenYellow"/>

    <SolidColorBrush x:Key="ButtonMouseOverBackgroundBrush" Color="DarkRed"/>
    <SolidColorBrush x:Key="ButtonIsPressedBackgroundBrush" Color="Black"/>
    <SolidColorBrush x:Key="ButtonIsPressedBorderBrush" Color="LemonChiffon"/>
    <SolidColorBrush x:Key="ButtonMouseOverBorderBrush" Color="SaddleBrown"/>

</ResourceDictionary>